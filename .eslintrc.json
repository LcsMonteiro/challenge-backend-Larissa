{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "parser": "babel-eslint",
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins":[
        "prettier"
    ],
    "rules": {
        "prettier/prettier":"error",
        "quotes": ["error", "double"],
        "no-console": "off",
        "arrow-body-style":"off"
        }
}
